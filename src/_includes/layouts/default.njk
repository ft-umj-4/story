{%- set metaTitle -%} {%- if not title -%}{{ site.title }} -
{{ site.description | safe }} {%- else -%}{{ title | safe }} |
{{ site.title }} {%- endif -%} {%- endset -%} {%- set metaDesc -%} {%- if not
description -%}{{ site.description | safe }} {%- else -%}{{
  description | safe
}}
{%- endif -%} {%- endset -%} {%- set metaKeywords -%} {%- if tags -%}{%- for tag
in tags -%}{{ tag }},{%- endfor -%}{%- endif -%} {%- if site.keywords -%} {%-
for keyword in site.keywords -%} {{ keyword }}{%- if not loop.last %},{% endif
-%} {%- endfor -%} {%- endif -%} {%- endset -%}
